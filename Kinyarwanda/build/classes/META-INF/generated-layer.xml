                        methodvalue="org.netbeans.api.settings.Factory.properties" name="settings.convertor"/>
                    <attr name="settings.instanceClass" stringvalue="Kinyarwanda.KinyarwandaTopComponent"/>
                    <attr name="settings.instanceOf" stringvalue="Kinyarwanda.KinyarwandaTopComponent"/>
