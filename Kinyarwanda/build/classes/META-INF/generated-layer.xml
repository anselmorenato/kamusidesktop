        <folder name="entities">
            <folder name="Kinyarwanda">
                <file name="Paldo" url="nbres:/org/netbeans/modules/settings/resources/properties-1_0.dtd">
                    <!--Kinyarwanda.KinyarwandaTopComponent-->
                    <attr name="hint.originalPublicID" stringvalue="-//Kinyarwanda//Paldo//EN"/>
