        </folder>
        <folder name="lookups">
            <folder name="Kinyarwanda">
                <file name="Paldo.instance">
                    <!--Kinyarwanda.KinyarwandaTopComponent-->
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.create" name="instanceCreate"/>
