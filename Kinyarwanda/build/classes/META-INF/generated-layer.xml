        </folder>
        <folder name="memory">
            <folder name="Kinyarwanda">
                <file name="KinyarwandaTopComponent">
                    <!--Kinyarwanda.KinyarwandaTopComponent-->
                    <attr name="settings.providerPath" stringvalue="xml/lookups//Kinyarwanda/Paldo.instance"/>
