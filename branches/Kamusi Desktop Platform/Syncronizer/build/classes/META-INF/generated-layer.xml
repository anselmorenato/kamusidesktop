<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN"
                            "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="xml">
        <folder name="entities">
            <folder name="org_kamusi_sync">
                <file name="Synchronizer" url="nbres:/org/netbeans/modules/settings/resources/properties-1_0.dtd">
                    <!--org.kamusi.sync.SynchronizerTopComponent-->
                    <attr name="hint.originalPublicID" stringvalue="-//org.kamusi.sync//Synchronizer//EN"/>
                </file>
            </folder>
        </folder>
        <folder name="memory">
            <folder name="org">
                <folder name="kamusi">
                    <folder name="sync">
                        <file name="SynchronizerTopComponent">
                            <!--org.kamusi.sync.SynchronizerTopComponent-->
                            <attr name="settings.providerPath" stringvalue="xml/lookups//org_kamusi_sync/Synchronizer.instance"/>
                        </file>
                    </folder>
                </folder>
            </folder>
        </folder>
        <folder name="lookups">
            <folder name="org_kamusi_sync">
                <file name="Synchronizer.instance">
                    <!--org.kamusi.sync.SynchronizerTopComponent-->
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.create" name="instanceCreate"/>
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.properties" name="settings.convertor"/>
                    <attr name="settings.instanceClass" stringvalue="org.kamusi.sync.SynchronizerTopComponent"/>
                    <attr name="settings.instanceOf" stringvalue="org.kamusi.sync.SynchronizerTopComponent"/>
                    <attr boolvalue="true" name="xmlproperties.preventStoring"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
