<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN"
                            "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="OptionsDialog">
        <folder name="Options">
            <file name="credentials-UsernameOptionsPanelController.instance">
                <!--credentials.UsernameOptionsPanelController-->
                <attr
                    methodvalue="org.netbeans.spi.options.AdvancedOption.createSubCategory" name="instanceCreate"/>
                <attr name="controller" newvalue="credentials.UsernameOptionsPanelController"/>
                <attr
                    bundlevalue="credentials.Bundle#AdvancedOption_DisplayName_Username" name="displayName"/>
                <attr
                    bundlevalue="credentials.Bundle#AdvancedOption_Keywords_Username" name="keywords"/>
                <attr name="keywordsCategory" stringvalue="Options/Username"/>
            </file>
            <!--org.kamusi.dictionary-->
            <attr intvalue="0" name="position"/>
        </folder>
        <file name="Options.instance">
            <!--org.kamusi.dictionary-->
            <attr
                methodvalue="org.netbeans.spi.options.OptionsCategory.createCategory" name="instanceCreate"/>
            <attr name="advancedOptionsFolder" stringvalue="OptionsDialog/Options"/>
            <attr
                bundlevalue="org.kamusi.dictionary.Bundle#OptionsCategory_Name_Options" name="categoryName"/>
            <attr name="iconBase" stringvalue="org/kamusi/dictionary/kamusi_32x32.png"/>
            <attr
                bundlevalue="org.kamusi.dictionary.Bundle#OptionsCategory_Keywords_Options" name="keywords"/>
            <attr name="keywordsCategory" stringvalue="Options"/>
        </file>
    </folder>
    <folder name="xml">
        <folder name="entities">
            <folder name="org_kamusi_dictionary">
                <file name="Search" url="nbres:/org/netbeans/modules/settings/resources/properties-1_0.dtd">
                    <!--org.kamusi.dictionary.SearchTopComponent-->
                    <attr name="hint.originalPublicID" stringvalue="-//org.kamusi.dictionary//Search//EN"/>
                </file>
            </folder>
        </folder>
        <folder name="memory">
            <folder name="org">
                <folder name="kamusi">
                    <folder name="dictionary">
                        <file name="SearchTopComponent">
                            <!--org.kamusi.dictionary.SearchTopComponent-->
                            <attr name="settings.providerPath" stringvalue="xml/lookups//org_kamusi_dictionary/Search.instance"/>
                        </file>
                    </folder>
                </folder>
            </folder>
        </folder>
        <folder name="lookups">
            <folder name="org_kamusi_dictionary">
                <file name="Search.instance">
                    <!--org.kamusi.dictionary.SearchTopComponent-->
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.create" name="instanceCreate"/>
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.properties" name="settings.convertor"/>
                    <attr name="settings.instanceClass" stringvalue="org.kamusi.dictionary.SearchTopComponent"/>
                    <attr name="settings.instanceOf" stringvalue="org.kamusi.dictionary.SearchTopComponent"/>
                    <attr boolvalue="true" name="xmlproperties.preventStoring"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
