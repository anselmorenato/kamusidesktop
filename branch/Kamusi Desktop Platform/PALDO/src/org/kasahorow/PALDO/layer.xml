        <folder name="Window">
            <file name="org-kasahorow-PALDO-PaldoAction.instance">
                <attr name="component" methodvalue="org.kasahorow.PALDO.PaldoTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.kasahorow.PALDO.Bundle#CTL_PaldoAction"/>
                <attr name="iconBase" stringvalue="org/kasahorow/PALDO/favicon.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="PaldoAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-kasahorow-PALDO-PaldoAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="PaldoTopComponent.settings" url="PaldoTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="PaldoTopComponent.wstcref" url="PaldoTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
